public class Solution {
    public int MaximumWealth(int[][] accounts) {
        int[] arr = new int[accounts.Length];
        for(int i = 0; i < accounts.Length; i++) {
            for(int j = 0; i < accounts.Length; i++) {
                arr[i] += accounts[i][j];
            }
        }
        Console.WriteLine(arr);
    }
}